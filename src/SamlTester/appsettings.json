{
  "KeyVaultName": "screeningui-kv-dev",
  "AppConfigurationName": "app-configuration-dev-env",
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "ComponentSpace": "Debug"
    }
  },
  "Urls": "https://localhost:5002",
  "AllowedHosts": "*",
  "ASPNETCORE_ENVIRONMENT": "Development",
  "SAML": {
    "$schema": "https://www.componentspace.com/schemas/saml-config-schema-v1.0.json",
    "Configurations": [
      {
        "LocalIdentityProviderConfiguration": {
          "Name": "https://local.idp.org/metadata/crisphealth.org",
          "Description": "CRISP Saml Tester Identity Provider",
          "LocalCertificates": [
            {
              "Key": "Sp"
            }
          ]
        },
        "PartnerServiceProviderConfigurations": [
          {
            "Name": "https://local.crisphealth.org/metadata/crisphealth.org",
            "Description": "Screening Service Provider",
            "SignSamlResponse": true,
            "EncryptAssertion": true,
            "AssertionConsumerServiceUrl": "http//localhost:5001/Saml/AssertionConsumerService",
            "PartnerCertificates": [
              {
                "Key": "Sp"
              }
            ]
          }
        ]
      }
    ]
  }
}