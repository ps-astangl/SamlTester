{
  "KeyVaultName": "screeningui-kv-dev",
  "AppConfigurationName": "app-configuration-dev-env",
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "ComponentSpace": "Debug"
    }
  },
  "Urls": "https://localhost:5002",
  "AllowedHosts": "*",
  "ASPNETCORE_ENVIRONMENT": "Development",
  "SAML": {
    "$schema": "https://www.componentspace.com/schemas/saml-config-schema-v1.0.json",
    "PartnerName": "https://localhost:5002/saml/AssertionConsumerService",
    "Configurations": [
      {
        "LocalIdentityProviderConfiguration": {
          "Name": "https://idp.local.com",
          "Description": "Local Identity Provider",
          "LocalCertificates": [
            {
              "Key": "Sp"
            }
          ]
        },
        "PartnerServiceProviderConfigurations": [
          {
            "Name": "Screening",
            "Description": "Local Service Provider",
            "SignAssertion": true,
            "EncryptAssertion": true,
            "AssertionConsumerServiceUrl": "https://localhost:5002/saml/AssertionConsumerService",
            "PartnerCertificates": [
              {
                "Key": "Sp"
              }
            ]
          },
          {
            "Name": "ReferralUI",
            "Description": "Local Service Provider",
            "SignAssertion": true,
            "EncryptAssertion": true,
            "AssertionConsumerServiceUrl": "https://localhost:5002/saml/AssertionConsumerService",
            "PartnerCertificates": [
              {
                "Key": "Sp"
              }
            ]
          }
        ]
      }
    ]
  }
}